{
  "name": "@gluestack-ui-nightly/core",
  "version": "1.0.0",
  "main": "lib/esm/index.js",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf lib",
    "build": "tsc -p tsconfig.build.json",
    "watch": "tsc -p tsconfig.build.json --watch",
    "dev": "nodemon --watch src --ext ts,tsx,js,jsx,json --exec 'yarn build && yalc push --private'"
  },
  "exports": {
    ".": {
      "import": "./lib/esm/index.js",
      "types": "./lib/esm/index.d.ts"
    },
    "./button/aria": {
      "import": "./lib/esm/button/aria/index.js",
      "types": "./lib/esm/button/aria/index.d.ts"
    },
    "./button/creator": {
      "import": "./lib/esm/button/creator/index.jsx",
      "types": "./lib/esm/button/creator/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./lib/esm/index.d.ts"
      ],
      "./button/aria": [
        "./lib/esm/button/aria/index.d.ts"
      ],
      "./button/creator": [
        "./lib/esm/button/creator/index.d.ts"
      ]
    }
  },
  "dependencies": {
    "@gluestack-ui-nightly/utils": "*"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-native": ">=0.64.0",
    "react-native-web": ">=0.19.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-native": "^0.72.0",
    "react": "^18.2.0",
    "react-native": "^0.72.0",
    "react-native-web": "^0.19.0",
    "typescript": "^5.8.3"
  }
}
