  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-native": ">=0.64.0",
    "react-native-web": ">=0.19.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-native": "^0.72.0",
    "react": "^18.2.0",
    "react-native": "^0.72.0",
    "react-native-web": "^0.19.0",
    "typescript": "^5.8.3"
