{
  "dependencies": {
    "chokidar": "^4.0.3",
    "solito": "^4.4.1",
    "typescript": "^5.8.3"
  },
  "scripts": {
    "dev": "tsx --watch scripts/dev.ts",
    "dev:starter-kit-next": "tsx --watch scripts/dev.ts --mapper=starter-kit-next",
    "dev:starter-kit-expo": "tsx --watch scripts/dev.ts --mapper=starter-kit-expo",
    "dev:kitchen-sink": "tsx --watch scripts/dev.ts --mapper=kitchen-sink",
    "dev:website": "tsx --watch scripts/dev.ts --mapper=website",
    "dev:todo-app": "tsx --watch scripts/dev.ts --mapper=todo-app",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,mdx}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md,mdx}\"",
    "build:website": "cd apps/website && yarn build"
  },
  "devDependencies": {
    "@tsconfig/react-native": "^3.0.5",
    "prettier": "^3.2.5",
    "tsx": "^4.19.3"
  }
}
