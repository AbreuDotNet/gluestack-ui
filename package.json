{
  "dependencies": {
    "chokidar": "^4.0.3",
    "solito": "^4.4.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3"
  },
  "scripts": {
    "dev": "tsx --watch scripts/dev.ts",
    "dev:starter-kit-next": "tsx --watch scripts/dev.ts --mapper=starter-kit-next",
    "dev:starter-kit-expo": "tsx --watch scripts/dev.ts --mapper=starter-kit-expo",
    "dev:kitchen-sink": "tsx --watch scripts/dev.ts --mapper=kitchen-sink",
    "dev:website": "tsx --watch scripts/dev.ts --mapper=website",
    "dev:todo-app": "tsx --watch scripts/dev.ts --mapper=todo-app",
    "sync": "tsx scripts/dev.ts --sync",
    "sync:starter-kit-next": "tsx scripts/dev.ts --mapper=starter-kit-next --sync",
    "sync:starter-kit-expo": "tsx scripts/dev.ts --mapper=starter-kit-expo --sync",
    "sync:kitchen-sink": "tsx scripts/dev.ts --mapper=kitchen-sink --sync",
    "sync:website": "tsx scripts/dev.ts --mapper=website --sync",
    "sync:website:alt": "ts-node scripts/dev.ts --mapper=website --sync",
    "sync:todo-app": "tsx scripts/dev.ts --mapper=todo-app --sync",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,mdx}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md,mdx}\"",
    "build:website": "cd apps/website && yarn build"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.5",
    "@tsconfig/react-native": "^3.0.5",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,json,md,mdx}": [
      "prettier --write"
    ]
  }
}
