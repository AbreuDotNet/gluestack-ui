{
  "dependencies": {
    "chokidar": "^4.0.3",
    "solito": "^4.4.1",
    "typescript": "^5.8.3"
  },
  "scripts": {
    "dev": "tsx --watch scripts/dev.ts",
    "dev:starter-kit-next": "tsx --watch scripts/dev.ts --mapper=starter-kit-next",
    "dev:starter-kit-expo": "tsx --watch scripts/dev.ts --mapper=starter-kit-expo",
    "dev:kitchen-sink": "tsx --watch scripts/dev.ts --mapper=kitchen-sink",
    "dev:website": "tsx --watch scripts/dev.ts --mapper=website",
    "dev:todo-app": "tsx --watch scripts/dev.ts --mapper=todo-app",
    "sync": "tsx --watch scripts/dev.ts --sync",
    "sync:starter-kit-next": "tsx --watch scripts/dev.ts --mapper=starter-kit-next --sync",
    "sync:starter-kit-expo": "tsx --watch scripts/dev.ts --mapper=starter-kit-expo --sync",
    "sync:kitchen-sink": "tsx --watch scripts/dev.ts --mapper=kitchen-sink --sync",
    "sync:website": "tsx --watch scripts/dev.ts --mapper=website --sync",
    "sync:todo-app": "tsx --watch scripts/dev.ts --mapper=todo-app --sync",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,mdx}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md,mdx}\"",
    "build:website": "cd apps/website && yarn build"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.27.1",
    "@changesets/cli": "^2.29.4",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@tsconfig/react-native": "^3.0.5",
    "jest": "^30.0.0",
    "jest-environment-jsdom": "^30.0.0",
    "prettier": "^3.2.5",
    "tsx": "^4.19.3"
  }
}
